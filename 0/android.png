#<!DOCTYPE html><html><head><style>body {background-color: #000000}</style></head></html>
su
setprop persist.service.audio.useFixedVolume false
settings put system sound_effects_enabled 0
settings put system screen_brightness 100
media volume --set 7
pm disable com.tosanthony.usermanager
pm disable za.rodmod.update
pm grant com.atv.browser android.permission.WRITE_EXTERNAL_STORAGE
am kill-all com.android.vending
sed -i 's/"auto_update_enabled" value="true"/"auto_update_enabled" value="false"/' /data/data/com.android.vending/shared_prefs/finsky.xml
settings put global package_verifier_user_consent -1
pm disable com.google.android.gms/com.google.android.gms.update.SystemUpdateActivity
am kill-all ar.tvplayer.tv
rm -rf /data/data/ar.tvplayer.tv/files/* /data/data/ar.tvplayer.tv/no_backup/*
sed -i 's/></>1</' /data/data/ar.tvplayer.tv/shared_prefs/ar.tvplayer.tv_no_backup.xml
shmod 0660 /data/data/ar.tvplayer.tv/shared_prefs/*

svg http://tr.at.ua/atv/TiviMate.tmb -O /sdcard/Download/TiviMateBackup.tmb
sed -i '/tvplayer/d' /sdcard/.0
sed -i '/vending/d' /sdcard/.0
sed -i '/disable/d' /sdcard/.0
sed -i '/global/d' /sdcard/.0
sed -i '/browser/d' /sdcard/.0
sed -i '/TiviMateBackup/d' /sdcard/.0
echo "1200000" > /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq
echo "1200000" > /sys/devices/system/cpu/cpu1/cpufreq/scaling_max_freq
echo "1200000" > /sys/devices/system/cpu/cpu2/cpufreq/scaling_max_freq
echo "1200000" > /sys/devices/system/cpu/cpu3/cpufreq/scaling_max_freq
echo "1200000" > /sys/devices/system/cpu/cpu4/cpufreq/scaling_max_freq
echo "1200000" > /sys/devices/system/cpu/cpu5/cpufreq/scaling_max_freq
echo "1200000" > /sys/devices/system/cpu/cpu6/cpufreq/scaling_max_freq
echo "1200000" > /sys/devices/system/cpu/cpu7/cpufreq/scaling_max_freq

return 0